{
  "name": "monkeyminds-tools",
  "version": "1.0.0",
  "description": "Static solutions server for MonkeyMinds custom web utilities",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "npm run clean && npm run build:server && npm run build:solutions",
    "build:server": "tsc",
    "build:solutions": "node scripts/build-solutions.js",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "vercel-build": "npm run build"
  },
  "keywords": ["express", "static", "solutions", "typescript", "monkeyminds"],
  "author": "MonkeyMinds",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}